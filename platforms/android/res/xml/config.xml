<?xml version='1.0' encoding='utf-8'?>
<widget id="com.clevelandmetroparks.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <name>Cleveland Metroparks</name>
    <description>
        Find parks, trails, and amenities in Cleveland with Cleveland Metropark's mobile app.
    </description>
    <author email="gregor@greeninfo.org" href="http://www.greeninfo.org/">
        GreenInfo Network
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
</widget>
